-- ~15s with 20
univ : { M$1 .. M$2 W$1 .. W$2 }

const Man :{} { M$1 .. M$2 }
const Woman : {} { W$1 .. W$2  }

var wife : {} Man->Woman
var husband : {} Woman->univ

invariant

-- husband in Woman -> lone Man
husband in Woman->Man
all w : Woman | lone w.husband


-- wife in Man -> lone Woman
wife in Man->Woman
all m : Man | lone m.wife

husband = ~wife

run
no husband
sometime some husband

